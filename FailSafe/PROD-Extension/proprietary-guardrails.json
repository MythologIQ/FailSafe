{
  "blockedPathSegments": [
    "proprietary",
    ".proprietary",
    "internal",
    ".internal",
    "private",
    ".private"
  ],
  "blockedPathPrefixes": [
    ".agent/staging/"
  ],
  "targets": [
    {
      "name": "prod-vscode",
      "path": "FailSafe/PROD-Extension/VSCode",
      "requiredIgnoreGlobs": [
        ".failsafe/**/proprietary/**",
        ".failsafe/**/.proprietary/**",
        ".failsafe/**/internal/**",
