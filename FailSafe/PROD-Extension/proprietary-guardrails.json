        ".failsafe/**/.private/**",
        ".agent/staging/**"
      ]
    },
    {
      "name": "prod-antigravity",
      "path": "FailSafe/PROD-Extension/Antigravity",
      "requiredIgnoreGlobs": [
        ".agent/**/proprietary/**",
        ".agent/**/.proprietary/**",
        ".agent/**/internal/**",
        ".agent/**/.internal/**",
        ".agent/**/private/**",
        ".agent/**/.private/**",
        ".agent/staging/**",
        ".qorelogic/**/proprietary/**",
        ".qorelogic/**/.proprietary/**",
        ".qorelogic/**/internal/**",
        ".qorelogic/**/.internal/**",
        ".qorelogic/**/private/**",
        ".qorelogic/**/.private/**"
      ]
    }
  ]
